{
  "name": "shiny-spork",
  "version": "1.1.0",
  "description": "Highlights Pull Requests status on GitHub.",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "package": " zip -r extension.zip ./extension/*",
    "build:firefox": "web-ext build --source-dir ./extension/",
    "lint": "eslint ./extension/src",
    "lint:fix": "eslint ./extension/src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/floragc-octo/shiny-spork.git"
  },
  "keywords": [
    "github",
    "git",
    "pull",
    "requests",
    "chrome",
    "firefox",
    "brave",
    "chrome extension",
    "firefox extension",
    "brave extension",
    "extension",
    "pr"
  ],
  "author": "Flora GC, Simon Belbeoch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/floragc-octo/shiny-spork/issues"
  },
  "homepage": "https://github.com/floragc-octo/shiny-spork#readme",
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "web-ext": "^5.4.0"
  }
}
